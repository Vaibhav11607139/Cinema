<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <Button
        android:id="@+id/ninetom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="135dp"
        android:background="@drawable/button2"
        android:text="9:15 AM"
        android:textColor="#e53bcf21" />
    <Button
        android:id="@+id/tentom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/textView"
        android:layout_marginBottom="135dp"
        android:layout_marginLeft="110dp"
        android:background="@drawable/button2"
        android:text="10:30 AM"
        android:textColor="#e53bcf21" />
    <Button
        android:id="@+id/onetom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/textView"
        android:layout_marginBottom="135dp"
        android:layout_marginLeft="215dp"
        android:background="@drawable/button2"
        android:text="1:30 PM"
        android:textColor="#e53bcf21" />
    <Button
        android:id="@+id/fivetom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginTop="60dp"
        android:background="@drawable/button2"
        android:text="05:15 PM"
        android:textColor="#e53bcf21" />
    <Button
        android:id="@+id/seventom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/textView"
        android:layout_marginTop="60dp"
        android:layout_marginLeft="110dp"
        android:background="@drawable/button2"
        android:text="7:30 PM"
        android:textColor="#e53bcf21" />



</FrameLayout>